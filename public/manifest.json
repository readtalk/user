  "shortcuts": [
    {
      "name": "New Chat",
      "short_name": "Chat",
      "description": "Start a new conversation",
      "url": "/?new-chat",
      "icons": [
        {
          "src": "/icons/shortcut-chat-96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "New Group",
      "short_name": "Group",
      "description": "Create a new group chat",
      "url": "/?new-group",
      "icons": [
        {
          "src": "/icons/shortcut-group-96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Camera",
      "short_name": "Camera",
      "description": "Open camera to take photo or video",
      "url": "/?camera",
      "icons": [
        {
          "src": "/icons/shortcut-camera-96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Status",
      "short_name": "Status",
      "description": "View status updates",
      "url": "/?status",
      "icons": [
        {
          "src": "/icons/shortcut-status-96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Calls",
      "short_name": "Calls",
      "description": "View call history",
      "url": "/?calls",
      "icons": [
        {
          "src": "/icons/shortcut-calls-96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  
  "protocol_handlers": [
    {
      "protocol": "web+whatsapp",
      "url": "/?handle=%s"
    },
    {
      "protocol": "whatsapp",
      "url": "/?handle=%s"
    }
  ],
  
  "share_target": {
    "action": "/?share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "images",
          "accept": ["image/*"]
        },
        {
          "name": "videos",
          "accept": ["video/*"]
        },
        {
          "name": "documents",
          "accept": [".pdf", ".doc", ".docx", ".xls", ".xlsx", ".ppt", ".pptx", ".txt"]
        }
      ]
    }
  },
  
  "file_handlers": [
    {
      "action": "/?open-file",
      "accept": {
        "application/pdf": [".pdf"],
        "application/msword": [".doc", ".docx"],
        "application/vnd.ms-excel": [".xls", ".xlsx"],
        "application/vnd.ms-powerpoint": [".ppt", ".pptx"],
        "text/plain": [".txt"],
        "image/*": [".jpg", ".jpeg", ".png", ".gif", ".webp"],
        "video/*": [".mp4", ".avi", ".mov", ".mkv"],
        "audio/*": [".mp3", ".wav", ".ogg", ".m4a"]
      }
    }
  ],
  
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  
  "edge_side_panel": {
    "preferred_width": 320
  },
  
  "permissions": [
    "notifications",
    "background-sync",
    "clipboard-write",
    "clipboard-read",
    "geolocation",
    "camera",
    "microphone"
  ],
  
  "related_applications": [
